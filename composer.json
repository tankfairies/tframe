{
  "name": "tankfairies/tframe",
  "description": "Micro MVC framework",
  "type": "project",
  "keywords": ["mvc", "framework"],
  "homepage": "https://tankfairies.com/",
  "license": "MIT",
  "authors": [
    {
      "name": "Jon",
      "email": "jon@tankfairies.com",
      "role": "lead"
    }
  ],
  "support": {
    "issues": "https://github.com/tankfairies/tframe/issues"
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "^7.2"
  },
  "autoload": {
    "psr-4": {
      "Tankfairies\\Tframe\\": "src/"
    }
  },
  "autoload-dev": {
  },
  "require-dev": {
  },
  "config": {
    "platform": {
      "php": "7.3.0"
    },
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "scripts": {
    "post-package-install": [
      "@installer"
    ],
    "installer": [
      "Tankfairies\\Tframe\\Installer::postPackageInstall"
    ]
  }
}
